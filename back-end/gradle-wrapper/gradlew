#!/usr/bin/env sh

##############################################################################
##
##  Gradle start up script for UN*X
##
##############################################################################

# Attempt to locate JAVA_HOME
if [ -z "$JAVA_HOME" ]; then
  JAVA_EXE=$(which java)
else
  JAVA_EXE="$JAVA_HOME/bin/java"
fi

# Run gradle wrapper
exec "$JAVA_EXE" -classpath "gradle/wrapper/gradle-wrapper.jar" org.gradle.wrapper.GradleWrapperMain "$@"
